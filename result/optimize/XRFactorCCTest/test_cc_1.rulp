(defun fun1 (?v)
    (if (<= ?v 0) 
        (return 0))
    (return (+ (fun1 (- ?v 1)) 1))
)
;=>

(defun fun2 (?m)
    (defvar ?x 0)
    (loop for ?i from 1 to ?m do
        (setq ?x (+ ?x (fun1 (+ ?i 1))))
    )
    (return ?x)
)
;=>

(cc (fun2 5))
;=>

(attribute-of fun1)
;=>

(attribute-of fun2)
;=>

(print-impl fun2)

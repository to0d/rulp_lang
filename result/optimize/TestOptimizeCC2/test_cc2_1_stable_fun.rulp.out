(defun fun1 (?v)
    (return (+ ?v 1))
)
;=>fun1

(defun fun2 (?m)
    (defvar ?x 0)
    (loop for ?i from 1 to ?m do
        (setq ?x (+ ?x (fun1 ?i)))
    )
    (return ?x)
)[opt-cc2]
;=>fun2

(attribute-of fun1)
;=>'()

(attribute-of fun2)
;=>'(opt-cc2)

(print-impl fun2)
;=>nil
;out:
(defun fun2 (?m)
    (do
        (defvar ?x 0)
        (loop for ?i from 1 to ?m do
            (setq ?x (+ ?x (cc2 (fun1 ?i))))
        )
        (return ?x)
    )
)


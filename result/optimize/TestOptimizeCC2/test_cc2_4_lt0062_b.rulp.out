            (return 0)
        )
        (if
            (< m n)
            (return (uniquePaths n m))
        )
        (if
            (= n 1)
            (return 1)
        )
        (return (+ (uniquePaths (- m 1) n) (uniquePaths m (- n 1))))
    )
)


;; fact(n) = n*fact(n-1)
(defun fact (?v)
    (if (<= ?v -1)(return -1))
    (return (* ?v (fact (- ?v 1))))
)[opt-tco opt-lco]
;=>fact

(fact 0)
;=>0

(attribute-of fact)
;=>'(opt-tco)

(print-impl fact)
;=>nil
;out:
(defun fact (?v)
    (do
        (if
            (<= ?v -1)
            (return -1)
        )
        (cps[opt-id=0] (* ?v (fact (- ?v 1))))
    )
)[opt-tco]

; (power 1 e2 e3 e4...) ==> 1

(reduct (power 1 a))
;=>1

(reduct (power 1.0 a))
;=>1

(reduct (power 1D a))
;=>1

(reduct (power 1L a))
;=>1

(reduct (power 1 a b c))
;=>1

; (power e1 e2 e3 0 e4) ==> 1

(reduct (power a 0))
;=>1

(reduct (power a 0.0))
;=>1

(reduct (power a 0D))
;=>1

(reduct (power a 0L))
;=>1

(reduct (power a 0 b c))
;=>1

(reduct (power a b 0 c))
;=>1

; (power e1 e2 e3 1 e4) ==> (power e1 e2 e3 e4)

(reduct (power a 1))
;=>a

(reduct (power a 1.0))
;=>a

(reduct (power a 1D))
;=>a

(reduct (power a 1L))
;=>a

(reduct (power a 1 b c))
;=>(power a (* b c))

; (power e1 e2 e3 m n e4) ==> (power e1 (* m n e2 e3 e4))

(reduct (power a 2 3))
;=>(power a 6)

(reduct (power a 3 b 4))
;=>(power a (* 12 b))

(reduct (power a 3 b 3 c))
;=>(power a (* 9 b c))


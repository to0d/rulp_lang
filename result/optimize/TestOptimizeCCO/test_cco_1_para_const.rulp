(defun fun1 (?v)
    (return (+ ?v 1))
)
;=>

(defun fun2 ()
    (defvar ?x 0)
    (loop for x from 1 to 3 do
        (setq ?x (+ ?x (fun1 1)))
    )
    (return ?x)
)[opt-cco]
;=>

(fun2)
;=>

(attribute-of fun1)
;=>

(attribute-of fun2)
;=>

(print-impl fun2)

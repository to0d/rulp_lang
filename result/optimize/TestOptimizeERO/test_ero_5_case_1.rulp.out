(defun fun (?v) 

    (if (<= ?v 0)
        (return 0))
        
    (case (+ 1 1)
        (1 (return (+ 1 (fun (- ?v 1)))))
        (2 (return (+ 2 (fun (- ?v 1)))))
        (? (return (+ 3 (fun (- ?v 1))))))

)[opt-ero]
;=>fun

(attribute-of fun)
;=>'(opt-ero)

(print-impl fun)
;=>nil
;out:
(defun fun (?v)
    (do[stable]
        (if (<= ?v 0)
            (return 0)
        )
        (return[stable] (+[atom return-type=1 stable] 2 (fun (- ?v 1))))
    )
)[opt-ero]


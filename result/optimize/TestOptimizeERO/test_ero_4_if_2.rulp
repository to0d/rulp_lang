(defun fun (?v) 

    (if (<= ?v 0) do
        (return 0)
    )
        
    (if (> 2 1) do
        (return (+ 1 (fun (- ?v 1))))        
    )

    (return (+ 2 (fun (- ?v 1))))

)[opt-ero]

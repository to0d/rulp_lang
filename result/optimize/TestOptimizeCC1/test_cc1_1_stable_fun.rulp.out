(defun fun1 (?v)
    (return (+ ?v 1))
)
;=>fun1

(defun fun2 ()
    (defvar ?x 0)
    (loop for x from 1 to 3 do
        (setq ?x (+ ?x (fun1 1)))
    )
    (return ?x)
)[opt-cc1]
;=>fun2

(attribute-of fun1)
;=>'()

(attribute-of fun2)
;=>'(opt-cc1)

(print-impl fun2)
;=>nil

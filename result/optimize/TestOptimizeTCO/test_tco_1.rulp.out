;; fact(n) = n*fact(n-1)
(defun fact (?v)
    (do
        (if (<= ?v 0)(return 1))
        (return (* ?v (fact (- ?v 1))))
    )
)[opt-tco]
;=>fact

(list-function-in-return fact)
;=>'("fact")

(attribute-of fact)
;=>'(opt-tco)

(fact 1)
;=>1

(fact 2)
;=>2

(fact 3)
;=>6

(fact 4)
;=>24

(tco-count)
;=>60

(_ginfo)
;=>nil


;; fact(n) = n*fact(n-1)
(defun fact (?v)
    (do
        (if (<= ?v 0)(return 1))
        (return (* ?v (fact (- ?v 1))))
    )
)[opt-tco]
;=>fact

(list-function-in-return fact)
;=>'("fact")

(attribute-of fact)
;=>'(opt-tco)

(print-impl fact)
;=>nil

;; fact(n) = n*fact(n-1)
(defun fact (?v)
    (do
        (if (<= ?v 0)(return 1))
        (return (* ?v (fact (- ?v 1))))
    )
)[op-cps]
;=>fact

(cps-callee fact)
;=>'("fact")

(attribute-of fact)
;=>'(op-cps)

(fact 1)
;=>1

(fact 2)
;=>2

(fact 3)
;=>6

(fact 4)
;=>24

(cps-count)
;=>60

(_ginfo)
;=>nil


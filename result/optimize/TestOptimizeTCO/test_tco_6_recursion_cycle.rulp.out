(setq ?op-cps true)
;=>&?op-cps

(defun fun4a (?v) 
    (do 
        (if (<= ?v 0)(return 0))
        (return (+ ?v (fun4b (- ?v 1))))
    )
)

(defun fun4b (?v) 
    (do 
        (if (<= ?v 0)(return 0))
        (return (+ ?v (fun4a (- ?v 1))))
    )
)
;=>fun4a fun4b

(cps-callee fun4a)
;=>'("+" "-" "fun4b")

(cps-callee fun4b)
;=>'("+" "-" "fun4a")

(fun4a 3)
;=>6

(cps-count)
;=>0


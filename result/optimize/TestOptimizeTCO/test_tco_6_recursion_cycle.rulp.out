(defun fun4a (?v) 
    (do 
        (if (<= ?v 0)(return 0))
        (return (+ ?v (fun4b (- ?v 1))))
    )
)[op-cps]

(defun fun4b (?v) 
    (do 
        (if (<= ?v 0)(return 0))
        (return (+ ?v (fun4a (- ?v 1))))
    )
)[op-cps]
;=>fun4a[op-cps] fun4b[op-cps]

(cps-callee fun4a)
;=>'("fun4b")

(attribute-of fun4a)
;=>'(op-cps)

(cps-callee fun4b)
;=>'("fun4a")

(attribute-of fun4b)
;=>'(op-cps)

(fun4a 3)
;=>6

(cps-count)
;=>0


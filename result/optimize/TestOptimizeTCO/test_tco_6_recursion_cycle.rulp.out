(defun fun4a (?v) 
    (do 
        (if (<= ?v 0)(return 0))
        (return (+ ?v (fun4b (- ?v 1))))
    )
)[opt-tco]

(defun fun4b (?v) 
    (do 
        (if (<= ?v 0)(return 0))
        (return (+ ?v (fun4a (- ?v 1))))
    )
)[opt-tco]
;=>fun4a fun4b

(list-function-in-return fun4a)
;=>'("fun4b")

(list-function-in-return fun4b)
;=>'("fun4a")


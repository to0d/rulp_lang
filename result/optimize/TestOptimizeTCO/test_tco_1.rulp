;; fact(n) = n*fact(n-1)
(defun fact (?v)
    (do
        (if (<= ?v 0)(return 1))
        (return (* ?v (fact (- ?v 1))))
    )
)[op-cps]
;=>

(cps-callee fact)
;=>

(attribute-of fact)
;=>

(fact 1)
;=>

(fact 2)

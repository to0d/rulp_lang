(defun fun1 (?x ?y)
    (if (<= ?x 0)
        (return 0))
    (return ?y)
)

(defun fun2 ((?x int) (?y int))
    (if (<= ?x 0)
        (return 0))
    (return ?y)
)

(defun fun3 ((?x int) (?y int))
    (if (<= ?x 0)
        (return 0))
    (return "abc")
)

(defun fun4 ((?x int) (?y int))
    (return (+ ?x ?y))
)

(defun fun5 ((?path string))    
    (defvar ?new-path ?path)
    (return ?new-path)
)

;=>


(return-type-of fun1)
;=>nil

(return-type-of fun2)
;=>int

(return-type-of fun3)
;=>nil

(return-type-of fun4)
;=>int

(return-type-of fun5)
;=>nil
(defun fun4a (?v) 
    (do 
        (if (<= ?v 0)(return 0))
        (return (+ ?v (fun4b (- ?v 1))))
    )
)

(defun fun4b (?v) 
    (do 
        (if (<= ?v 0)(return 0))
        (return (+ ?v (fun4a (- ?v 1))))
    )
)
;=>fun4a fun4b

(is-stable fun4a)
;=>true

(is-stable fun4b)
;=>true


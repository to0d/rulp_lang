(defun fun1 (?v) (return (+ ?v 1)))
;=>fun1

(name-of fun1)
;=>"(fun1 nil)"

(fun1 1)
;=>2

(defun fun1 (?v ?x) (return (+ ?v ?x)))
;=>fun1

(name-of fun1)
;=>"'((fun1 nil nil) (fun1 nil))"

(script_out (print-subject 1) "_a.txt")
;=>nil

(do
    (defun fun1 (?v) (return (+ ?v 2)))   
    (script_out (print-subject 2) "_b.txt")
    (fun1 1)
)
;=>nil

(fun1 1)
;=>2


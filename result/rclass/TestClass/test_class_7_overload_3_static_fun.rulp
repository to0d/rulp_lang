(defclass class1
    (defun fun1 (?x) (return (+ ?x 1)))[static]
)
(defclass class2 
    (extends class1)
    (defun fun1 (?x) (return (+ ?x 2)))[static]
)
;;;

(script_out (print-subject class2) "_a.txt")
;;;

(new class1 o1)
;;;

(o1::fun1 1)
;;;

(script_out (print-subject o1 -1) "_b.txt")
;;;

(new class2 o2)
;;;

(o2::fun1 1)
;;;

(script_out (print-subject o2 -1) "_c.txt")
;;;

(_ginfo)

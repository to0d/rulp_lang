(defclass class1 ()
    (defvar ?value 0)
    (defvar ?v1 1)
    (defvar ?v2 2)[final]
    (defun add (?x) (setq ?value (+ ?value ?x)))[final]
    (defun sub (?x) (setq ?value (- ?value ?x)))
    (defun get ()   (return ?value))
    (defun set (?x) (setq ?value ?x))
)

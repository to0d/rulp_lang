(defclass class1 ()
    (defun fun1 (?x) (return (+ ?x 1)))
)
(defclass class2 (class1)
    (defun fun1 (?x) (return (+ ?x 2)))
)
;;;

(print-subject class2 -1)
;;;

(new class1 o1)
;;;

(o1::fun1 1)
;;;

(print-subject o1 -1)
;;;

(new class2 o2)
;;;

(o2::fun1 1)
;;;

(print-subject o2 -1)
;;;

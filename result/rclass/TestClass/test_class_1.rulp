(defclass class1 ()
    (defvar ?value 0)
    (defun add (?x) (setq ?value (+ ?value ?x)))
    (defun sub (?x) (setq ?value (- ?value ?x)))
    (defun get ()   (return ?value))
    (defun set (?x) (setq ?value ?x))
)

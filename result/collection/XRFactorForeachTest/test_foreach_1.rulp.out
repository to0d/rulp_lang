(foreach (?a '(1 2 3)) (+ ?a 1))
;=>'(2 3 4)

(foreach (?a '(1 2 3)) (+ ?a 1) (+ ?a 2))
;=>'(3 4 5)

(foreach (?a '(1 2 3 4 5)) (if (> ?a 3) (return 1)) (return ?a))
;=>'(1 2 3 1 1)

(foreach (?a '(1 2 3 4 5)) (if (= ?a 3) (continue)) (return ?a))
;=>'(1 2 4 5)

(foreach (?a '(1 2 3 4 5)) (if (= ?a 3) (break)) (return ?a))
;=>'(1 2)

(foreach (?a '(1 2 3)) (return '(?a 1)))
;=>'('(1 1) '(2 1) '(3 1))

